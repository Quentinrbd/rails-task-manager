<div class="container">
  <h2>Add new task</h2>
  <%= form_with model: @task do |f| %>
  <div class="mb-3">
      <%= f.label :title %>
      <%= f.text_field :title %>
  </div>
  <div class="mb-3">
    <%= f.label :details %>
    <%= f.text_field :details %>
  </div>
    <div class="myBtn"><%= f.submit %></div>
  <% end %>
</div>
